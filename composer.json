{
    "name": "petty/tcpclient",
    "description": "A tcp client written by php.",
    "keywords": ["tcp", "client", "asynchronous"],
    "type": "library",
    "homepage": "https://github.com/php-petty/tcp-client",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "pengzhile",
            "email": "pengzhile@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {"Petty\\TcpClient\\": "src/"}
    },
    "autoload-dev": {
